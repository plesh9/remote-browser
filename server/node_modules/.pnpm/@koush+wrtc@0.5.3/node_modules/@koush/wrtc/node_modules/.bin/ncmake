#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/plesh9/Desktop/web/2024/remote-browser/server/node_modules/.pnpm/node-cmake@2.3.2/node_modules/node-cmake/lib/node_modules:/Users/plesh9/Desktop/web/2024/remote-browser/server/node_modules/.pnpm/node-cmake@2.3.2/node_modules/node-cmake/node_modules:/Users/plesh9/Desktop/web/2024/remote-browser/server/node_modules/.pnpm/node-cmake@2.3.2/node_modules:/Users/plesh9/Desktop/web/2024/remote-browser/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/plesh9/Desktop/web/2024/remote-browser/server/node_modules/.pnpm/node-cmake@2.3.2/node_modules/node-cmake/lib/node_modules:/Users/plesh9/Desktop/web/2024/remote-browser/server/node_modules/.pnpm/node-cmake@2.3.2/node_modules/node-cmake/node_modules:/Users/plesh9/Desktop/web/2024/remote-browser/server/node_modules/.pnpm/node-cmake@2.3.2/node_modules:/Users/plesh9/Desktop/web/2024/remote-browser/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node-cmake@2.3.2/node_modules/node-cmake/lib/ncmake.js" "$@"
else
  exec node  "$basedir/../../../../../../node-cmake@2.3.2/node_modules/node-cmake/lib/ncmake.js" "$@"
fi
